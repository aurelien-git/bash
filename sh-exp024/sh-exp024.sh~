#!/bin/bash
# A shell script that evaluate and send back the evaluation
# if else then fi echo read evaluation -le -ge + and operator -a


tput clear

echo "Enter a number between 5 to 50"
read num
if [ $num -le 50 -a $num -ge 5 ] # -a (and operator)
then
    echo "Correct!"
else
    echo "The number you enter is out of limit"
fi


# End of program
exit 0
